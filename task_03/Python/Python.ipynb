      "source": [
        "n = int(input(\"Enter a number (n): \"))\n",
        "list = []\n",
        "for j in range(2, n+1):\n",
        "    flag = True\n",
        "    for i in range(2, int(j**0.5) + 1):\n",
        "        if j % i == 0:\n",
        "            flag = False\n",
        "            break\n",
        "    if flag:\n",
        "        list.append(j)\n",
        "print(f\"The prime numbers up to {n} are:\")\n",
        "print(list)"
      ],
